@*@model Analytics.Models.UserInfo*@

@{
    ViewBag.Title = "g0.pe";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

   

    <script type="text/javascript">
        window.onload = function () {
            function success(position) {
                var latitude = position.coords.latitude;
                var longitude = position.coords.longitude;
                window.location.href = "/Home/LoginRid?latitude=" + latitude + "&longitude=" + longitude;
            }
            function error() {
                var latitude = "";
                var longitude = "";
                window.location.href = "/Home/LoginRid?latitude=0&longitude=0";
            }
            navigator.geolocation.getCurrentPosition(success, error);
        }


    </script>
    <meta name="viewport" content="width=device-width" />
    <title>GPS</title>
</head>
<body>
    <div>
    </div>
</body>
</html>

